target C {
    platform: {
      name: "rp2040",
      board: "pololu_3pi_2040_robot"
    },
    threading: false,
  }
  
reactor AngleToDistance{
    input encoder_reading:float
    output encoder_dist_cm:float
    reaction(encoder_reading) -> encoder_dist_cm {=
        float dist = 3.175f*3.1415f*encoder_reading->value/360.0f;
        lf_set(encoder_dist_cm, dist);
    =}
}